# /etc/profile: system-wide .profile file for the Bourne shell (sh(1))
# and Bourne compatible shells (bash(1), ksh(1), ash(1), ...).

PATH="/sbin:/usr/sbin:/usr/bin:/bin"
HOSTNAME=`/bin/hostname`
export TERM=vt100

export  PS1="\u@\h:\w\\ # "
export  PS2='> '

export PATH
export HOSTNAME

umask 022
