function  ReSetPVCRmZero(ID)
{
var pvc = getValue(ID);
var parts = pvc.split("/");
var msg;
var obj;
if(parts.length!=2)
{
return false;
}
for(i = 0;i < parts.length; i++)
{
obj = parts[i];
if(obj != null &&  obj != "")
{
while(obj.indexOf("0") == 0 && obj.length > 1)
{
obj = obj.substr(1);
}
parts[i]=obj;
}
if(i==0)
{
msg = parts[i];
}
else
{
var msg = parts[i-1]+"/"+parts[i];
}
}
setValue(ID,msg);
}
