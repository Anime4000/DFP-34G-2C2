}
else
{
jslEnDisplay("TR_WANCType","TABLE_static");
jslDiDisplay("TABLE_PPP","TR_PPPWANCType");
PageGetIPMode();
Change_WANCType();
}
setValue("Frm_IpMode","IPv4");
if("PPP" == linkmode)
{
PageGetPPPMode();
PageGetDialup("IPv4");
Change_PPPWANCType();
