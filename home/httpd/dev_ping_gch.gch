<%
IMPORT FILE "common_gch.gch";
var FP_ERRORSTR     = "SUCC";
var FP_OBJNAME      = "OBJ_DEVPING_ID";
var FP_PARANUM      = 13;
var FP_MULTIDISPLAY = 0;
var FP_TIME         = 8;
var FP_IDLE         = "";
var FP_HANDLE;
var PARA[13] =
{
"Host",
"NumofRepeat",
"DataBlockSize",
"PingAck",
"MinimumResponseTime",
"MaximumResponseTime",
"AverageResponseTime",
"SuccessCount",
"FailureCount",
"DiagnosticsState",
"DSCP",
"Interface",
"Timeout"
};
var timer=request("IF_TIME");
if(timer!=""&&timer!=NULL)
{
FP_TIME  = request("IF_TIME");
}
var idle = request("IF_IDLE");
if(idle=="submit")
{
FP_IDLE=idle;
}
var FP_ACTION = request("IF_ACTION");
create_form_start("fSubmit", "");
createSingleInstHidden(PARA, FP_PARANUM);
FP_ERRORSTR = applyOrNewOrDelInst(FP_OBJNAME, FP_ACTION, "", PARA, FP_PARANUM, FP_ERRORSTR);
undoDBSave();
function Pingcover_hidden(handle, PARA[], num)
{
for(var i = 0; i < num ; i ++)
{
if(PARA[i]=="PingAck")
{
PARA[i]="22";
}
else
{
="<script language=javascript>Transfer_meaning('";
=PARA[i];
="','";
=encodeJS(get_para(handle, PARA[i]));
="');";
="</script>\n";
}
}
}
if (FP_MULTIDISPLAY == 0)
{
FP_HANDLE = create_paralist();
get_inst(FP_HANDLE, FP_OBJNAME, "");

Pingcover_hidden(FP_HANDLE, PARA, FP_PARANUM);
destroy_paralist(FP_HANDLE);
}
getDisplayInstError(FP_ERRORSTR);
create_hidden_sep("IF_TIME", FP_TIME);
create_hidden_sep("IF_IDLE", FP_IDLE);
create_form_end();
%>
